@model List<ADODotNetExample.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary </td>
        </tr>
    }
</table>