@model List<TeamJhakas_30_09_2021.Models.EmployeeModel>
@{
    ViewBag.Title = "sendDatausingMultipleModel";
}

<h2>sendDatausingMultipleModel</h2>

<table class="table table-bordered table-condensed table-responsive table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>